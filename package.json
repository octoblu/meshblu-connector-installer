{
  "name": "meshblu-connector-installer-macos",
  "version": "1.3.0",
  "description": "Generate meshblu-connector installers for MacOS",
  "main": "index.js",
  "scripts": {
    "precommit": "lint-staged",
    "test": "mocha",
    "test:watch": "mocha -w -R mocha-multi --reporter-options spec=-,mocha-osx-reporter=-",
    "coverage": "nyc npm test",
    "mocha": "env NPM_ENV=test mocha --reporter json > coverage/mocha.json"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --no-semi --trailing-comma es5 --print-width 180",
      "git add"
    ]
  },
  "bin": {
    "meshblu-connector-installer-macos": "./command.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octoblu/meshblu-connector-installer-macos.git"
  },
  "keywords": [
    "meshblu",
    "meshblu-connector",
    "installer"
  ],
  "author": "Octoblu <npm@octoblu.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/octoblu/meshblu-connector-installer-macos/issues"
  },
  "homepage": "https://github.com/octoblu/meshblu-connector-installer-macos#readme",
  "dependencies": {
    "@octoblu/osx-pkg": "^1.1.0",
    "appdmg": "^0.4.5",
    "axios": "^0.16.2",
    "bluebird": "^3.5.0",
    "dashdash": "^1.14.1",
    "fs-extra": "^3.0.1",
    "json-templates": "^1.8.0",
    "ora": "^1.2.0",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "husky": "^0.13.4",
    "lint-staged": "^3.6.0",
    "mocha": "^3.4.2",
    "mocha-multi": "^0.11.0",
    "mocha-osx-reporter": "^0.1.2",
    "nyc": "^11.0.2",
    "prettier": "^1.4.2"
  },
  "nyc": {
    "cache": true,
    "reporter": [
      "text-summary",
      "lcov",
      "json"
    ],
    "extension": [
      ".coffee"
    ]
  },
  "engineStrict": true,
  "engines": {
    "node": ">=8.0.0"
  }
}
